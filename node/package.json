            "node": true,
            "commonjs": true,
            "es6": true,
            "browser": true,
            "jest": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:vue/essential"
        ],
        "globals": {
            "Atomics": "readonly",
            "SharedArrayBuffer": "readonly",
            "Vue": true,
            "validator": true
        },
        "parserOptions": {
            "ecmaVersion": 2018
